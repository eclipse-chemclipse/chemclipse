<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.chemclipse.msd.converter.chromatogramSupplier">
      <ChromatogramSupplier
            description="Reads and writes Open Chromatography binary chromatograms"
            exportConverter="org.eclipse.chemclipse.msd.converter.supplier.ocx.converter.ChromatogramExportConverter"
            fileExtension=".ocb"
            filterName="Open Chromatography Binary (*.ocb)"
            id="org.eclipse.chemclipse.xxd.converter.supplier.chemclipse"
            importContentMatcher="org.eclipse.chemclipse.msd.converter.supplier.ocx.converter.FileContentMatcherMSD"
            importConverter="org.eclipse.chemclipse.msd.converter.supplier.ocx.converter.ChromatogramImportConverter"
            importMagicNumberMatcher="org.eclipse.chemclipse.xxd.converter.supplier.ocx.converter.MagicNumberMatcher"
            isExportable="true"
            isImportable="true">
      </ChromatogramSupplier>
   </extension>
   <extension
         point="org.eclipse.chemclipse.msd.converter.peakSupplier">
      <PeakSupplier
            description="Reads peaks from Open Chromatography chromatograms"
            exportConverter="org.eclipse.chemclipse.msd.converter.supplier.ocx.converter.PeakExportConverter"
            fileExtension=".ocb"
            filterName="Open Chromatography Binary (*.ocb)"
            id="org.eclipse.chemclipse.xxd.converter.supplier.chemclipse.peaks"
            importConverter="org.eclipse.chemclipse.msd.converter.supplier.ocx.converter.PeakImportConverter"
            isExportable="false"
            isImportable="true">
      </PeakSupplier>
   </extension>
   <extension
         point="org.eclipse.chemclipse.csd.converter.chromatogramSupplier">
      <ChromatogramSupplier
            description="Reads and writes Open Chromatography chromatograms"
            exportConverter="org.eclipse.chemclipse.csd.converter.supplier.ocx.converter.ChromatogramExportConverter"
            fileExtension=".ocb"
            filterName="Open Chromatography Binary (*.ocb)"
            id="org.eclipse.chemclipse.xxd.converter.supplier.chemclipse"
            importContentMatcher="org.eclipse.chemclipse.csd.converter.supplier.ocx.converter.FileContentMatcherCSD"
            importConverter="org.eclipse.chemclipse.csd.converter.supplier.ocx.converter.ChromatogramImportConverter"
            importMagicNumberMatcher="org.eclipse.chemclipse.xxd.converter.supplier.ocx.converter.MagicNumberMatcher"
            isExportable="true"
            isImportable="true">
      </ChromatogramSupplier>
   </extension>   
   <extension
         point="org.eclipse.chemclipse.wsd.converter.chromatogramSupplier">
      <ChromatogramSupplier
            description="Reads and writes Open Chromatography chromatograms"
            exportConverter="org.eclipse.chemclipse.wsd.converter.supplier.ocx.converter.ChromatogramExportConverter"
            fileExtension=".ocb"
            filterName="Open Chromatography Binary (*.ocb)"
            id="org.eclipse.chemclipse.xxd.converter.supplier.chemclipse"
            importContentMatcher="org.eclipse.chemclipse.wsd.converter.supplier.ocx.converter.FileContentMatcherWSD"
            importConverter="org.eclipse.chemclipse.wsd.converter.supplier.ocx.converter.ChromatogramImportConverter"
            importMagicNumberMatcher="org.eclipse.chemclipse.xxd.converter.supplier.ocx.converter.MagicNumberMatcher"
            isExportable="true"
            isImportable="true">
      </ChromatogramSupplier>
   </extension>
   <extension
         point="org.eclipse.chemclipse.converter.processMethodSupplier">
      <ProcessMethodSupplier
            description="Reads and writes process methods."
            exportConverter="org.eclipse.chemclipse.xxd.converter.supplier.ocx.methods.MethodExportConverter"
            fileExtension=".ocm"
            filterName="Process Method (*.ocm)"
            id="org.eclipse.chemclipse.xxd.converter.supplier.chemclipse.processMethodSupplier"
            importConverter="org.eclipse.chemclipse.xxd.converter.supplier.ocx.methods.MethodImportConverter"
            importMagicNumberMatcher="org.eclipse.chemclipse.xxd.converter.supplier.ocx.methods.MagicNumberMatcher"
            importContentMatcher="org.eclipse.chemclipse.xxd.converter.supplier.ocx.methods.FileContentMatcher"
            isExportable="true"
            isImportable="true">
      </ProcessMethodSupplier>
   </extension>
</plugin>